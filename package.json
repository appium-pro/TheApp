{
  "name": "TheApp",
  "version": "1.12.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appId=com.appiumpro.the_app --main-activity=com.appiumpro.the_app.MainActivity",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "mocha ./test/**/*.test.ts",
    "reinstall": "rm -rf node_modules && npm install",
    "update": "rm -rf package-lock.json && npm run reinstall && pod install --project-directory=ios",
    "android-release": "cd android && ./gradlew bundleRelease && ./gradlew assembleRelease",
    "ios-release": "react-native run-ios --configuration=Release"
  },
  "dependencies": {
    "@ernestbies/react-native-android-sms-listener": "^1.0.2",
    "@react-native-async-storage/async-storage": "^1.18.0",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-picker/picker": "^2.4.9",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-navigation": "^7.32.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-webview": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.2",
    "@types/bluebird": "^3.5.38",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "axios": "^1.4.0",
    "eslint": "^8.19.0",
    "expect": "^29.5.0",
    "metro-react-native-babel-preset": "0.77.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webdriverio": "^8.10.6"
  },
  "engines": {
    "node": ">=16"
  }
